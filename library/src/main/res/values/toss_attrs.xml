<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TossAnimation">
        <attr name="circleCount" format="integer" />
        <attr name="xAxisDirection">
            <flag name="none" value="0" />
            <flag name="clockwise" value="1" />
            <flag name="abtucclockwise" value="-1" />
        </attr>
        <attr name="yAxisDirection">
            <flag name="none" value="0" />
            <flag name="clockwise" value="1" />
            <flag name="abtucclockwise" value="-1" />
        </attr>
        <attr name="zAxisDirection">
            <flag name="none" value="0" />
            <flag name="clockwise" value="1" />
            <flag name="abtucclockwise" value="-1" />
        </attr>
        <attr name="result">
            <flag name="front" value="1" />
            <flag name="reverse" value="-1" />
        </attr>
        <attr name="frontDrawable" format="reference"/>
        <attr name="reverseDrawable" format="reference"/>
        <attr name="duration" format="integer"/>
        <attr name="startOffset" format="integer"/>
    </declare-styleable>
</resources>